generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model User {
  id                   String   @id @default(uuid())
  email                String?
  audio                String   @default("free")
  video                String   @default("free")
  warpstagram          String   @default("free")
  machine_1_name       String?
  machine_2_name       String?
  machine_3_name       String?
  machine_1_id         String?
  machine_2_id         String?
  machine_3_id         String?
  isEULAaccepted       Boolean  @default(false)
  audioDownloadsCount  Int      @default(0)
  videoDownloadsCount  Int      @default(0)
  warpstagramDownloads Int      @default(0)
  audioAuthCode        String   @default("")
  videoAuthCode        String   @default("")
  warpstagramAuthCode  String   @default("")
  createdAt            DateTime @default(now())
  updatedAt            DateTime @updatedAt
}
