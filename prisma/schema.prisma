generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model User {
  id           String   @id @default(uuid())
  email        String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  machine_1_id String?
